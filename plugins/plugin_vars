# Plugin-specific qmake variables, set appropriately for platform and using 
# qmake_vars
# DO NOT DELETE this file, as it is NOT generated by the configure script

include("../qmake_vars")
unix{
    PLUGIN_DIR = $${MAIN_PATH}/lib/biblememorizer/plugins
}
mac{
    PLUGIN_DIR = $${MAIN_PATH}/Plugins
}
win32{
    PLUGIN_DIR = $${MAIN_PATH}/plugins
    DEFINES += Q_WS_WIN \
               Q_OS_WIN
    CONFIG += dll
}
