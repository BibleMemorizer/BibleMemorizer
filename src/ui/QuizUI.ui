<!DOCTYPE UI><UI version="3.3" stdsetdef="1">
<class>QuizUI</class>
<widget class="QWidget">
    <property name="name">
        <cstring>QuizUI</cstring>
    </property>
    <property name="geometry">
        <rect>
            <x>0</x>
            <y>0</y>
            <width>603</width>
            <height>480</height>
        </rect>
    </property>
    <property name="caption">
        <string>Form1</string>
    </property>
    <grid>
        <property name="name">
            <cstring>unnamed</cstring>
        </property>
        <widget class="QLabel" row="0" column="1" rowspan="1" colspan="2">
            <property name="name">
                <cstring>mVerseLabel</cstring>
            </property>
            <property name="sizePolicy">
                <sizepolicy>
                    <hsizetype>7</hsizetype>
                    <vsizetype>5</vsizetype>
                    <horstretch>0</horstretch>
                    <verstretch>0</verstretch>
                </sizepolicy>
            </property>
            <property name="text">
                <string></string>
            </property>
        </widget>
        <widget class="QLabel" row="0" column="0">
            <property name="name">
                <cstring>mVerseLeadLabel</cstring>
            </property>
            <property name="sizePolicy">
                <sizepolicy>
                    <hsizetype>4</hsizetype>
                    <vsizetype>5</vsizetype>
                    <horstretch>0</horstretch>
                    <verstretch>0</verstretch>
                </sizepolicy>
            </property>
            <property name="text">
                <string>Verse:</string>
            </property>
        </widget>
        <widget class="QLabel" row="1" column="0" rowspan="1" colspan="6">
            <property name="name">
                <cstring>mEntryLeadLabel</cstring>
            </property>
            <property name="sizePolicy">
                <sizepolicy>
                    <hsizetype>7</hsizetype>
                    <vsizetype>5</vsizetype>
                    <horstretch>0</horstretch>
                    <verstretch>0</verstretch>
                </sizepolicy>
            </property>
            <property name="text">
                <string>Enter the verse text below:</string>
            </property>
        </widget>
        <widget class="QTextEdit" row="2" column="0" rowspan="1" colspan="6">
            <property name="name">
                <cstring>mVerseEdit</cstring>
            </property>
        </widget>
        <widget class="QPushButton" row="3" column="0" rowspan="1" colspan="2">
            <property name="name">
                <cstring>mCheckButton</cstring>
            </property>
            <property name="text">
                <string>&amp;Check</string>
            </property>
            <property name="accel">
                <string>Alt+C</string>
            </property>
        </widget>
        <widget class="QPushButton" row="3" column="2">
            <property name="name">
                <cstring>mCorrectButton</cstring>
            </property>
            <property name="text">
                <string>Co&amp;rrect</string>
            </property>
            <property name="accel">
                <string>Alt+R</string>
            </property>
        </widget>
        <widget class="QPushButton" row="3" column="3">
            <property name="name">
                <cstring>mClearButton</cstring>
            </property>
            <property name="text">
                <string>Cle&amp;ar</string>
            </property>
            <property name="accel">
                <string>Alt+A</string>
            </property>
        </widget>
        <widget class="QLabel" row="3" column="4">
            <property name="name">
                <cstring>mResultLabel</cstring>
            </property>
            <property name="sizePolicy">
                <sizepolicy>
                    <hsizetype>7</hsizetype>
                    <vsizetype>5</vsizetype>
                    <horstretch>0</horstretch>
                    <verstretch>0</verstretch>
                </sizepolicy>
            </property>
            <property name="text">
                <string></string>
            </property>
        </widget>
        <widget class="QPushButton" row="3" column="5">
            <property name="name">
                <cstring>mNextButton</cstring>
            </property>
            <property name="enabled">
                <bool>false</bool>
            </property>
            <property name="text">
                <string>&amp;Next</string>
            </property>
            <property name="accel">
                <string>Alt+N</string>
            </property>
        </widget>
    </grid>
</widget>
<connections>
    <connection>
        <sender>mCheckButton</sender>
        <signal>clicked()</signal>
        <receiver>QuizUI</receiver>
        <slot>mCheckButton_clicked()</slot>
    </connection>
    <connection>
        <sender>mCorrectButton</sender>
        <signal>clicked()</signal>
        <receiver>QuizUI</receiver>
        <slot>mCorrectButton_clicked()</slot>
    </connection>
    <connection>
        <sender>mClearButton</sender>
        <signal>clicked()</signal>
        <receiver>QuizUI</receiver>
        <slot>mClearButton_clicked()</slot>
    </connection>
    <connection>
        <sender>mNextButton</sender>
        <signal>clicked()</signal>
        <receiver>QuizUI</receiver>
        <slot>mNextButton_clicked()</slot>
    </connection>
</connections>
<slots>
    <slot>mCheckButton_clicked()</slot>
    <slot>mCorrectButton_clicked()</slot>
    <slot>mClearButton_clicked()</slot>
    <slot>mNextButton_clicked()</slot>
</slots>
<layoutdefaults spacing="6" margin="11"/>
</UI>
